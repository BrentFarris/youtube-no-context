{
	"name": "YouTube No Context",
	"action": {
		"default_icon": "icon.png"
	},
	"manifest_version": 3,
	"version": "1.0",
	"description": "This removes YouTube's context/clarify box",
	"permissions": [
		"tabs",
		"scripting"
	],
	"host_permissions": [
		"https://*.youtube.com/*"
	],
	"background": {
		"service_worker": "background.js"
	}
}